	<subDataset name="Table Dataset 1" uuid="5a8661c4-f89c-461f-8df4-1a1045dac2ff">
		<parameter name="Professor" class="java.lang.String"/>
		<parameter name="Ano" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
     agendamento.`ano` AS agendamento_ano,
     ficha_identificacao.`orientador_id_professor` AS ficha_identificacao_orientador_id_professor,
     agendamento.`data_defesa` AS agendamento_data_defesa,
     professor.`nome` AS professor_nome,
     ficha_identificacao.`titulo_trabalho` AS ficha_identificacao_titulo_trabalho,
     usuario.`nome` AS usuario_nome
FROM
     `ficha_identificacao` ficha_identificacao INNER JOIN `agendamento` agendamento ON ficha_identificacao.`id` = agendamento.`ficha_identificacao_id`
     INNER JOIN `professor` professor ON ficha_identificacao.`avaliador2_id_professor` = professor.`id_professor`
     AND professor.`id_professor` = ficha_identificacao.`orientador_id_professor`
     AND professor.`id_professor` = ficha_identificacao.`avaliador1_id_professor`
     INNER JOIN `aluno` aluno ON ficha_identificacao.`aluno_id_usuario` = aluno.`id_usuario`
     INNER JOIN `usuario` usuario ON aluno.`id_usuario` = usuario.`id_usuario`]]>
		</queryString>
